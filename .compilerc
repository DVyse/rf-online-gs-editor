{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "2.0"
              }
            }
          ],
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-async-to-generator",
          "styled-components",
          "react-hot-loader/babel"
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "2.0"
              }
            }
          ],
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-react-constant-elements",
          "styled-components"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
