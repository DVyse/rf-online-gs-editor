{
  "env": {
    "development": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "3.0.7"
              }
            }
          ],
          "react",
          "stage-0"
        ],
        "plugins": [
          "transform-async-to-generator",
          "styled-components",
          "react-hot-loader/babel",
          ["module-resolver", {
            "alias": {
              "~": "./src"
            }
          }]
        ],
        "sourceMaps": "inline"
      }
    },
    "production": {
      "application/javascript": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "3.0.7"
              }
            }
          ],
          "react",
          "stage-0"
        ],
        "plugins": [
          ["module-resolver", {
            "alias": {
              "~": "./src"
            },
            "cwd": "packagejson"
          }],
          "transform-react-constant-elements",
          "transform-react-pure-class-to-function",
          "transform-react-remove-prop-types",
          "transform-react-inline-elements",
          "transform-async-to-generator",
          "styled-components"
        ],
        "sourceMaps": "none"
      }
    }
  }
}
